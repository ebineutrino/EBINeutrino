FROM (CRMINVOICE as inv LEFT JOIN CRMINVOICEPOSITION as pos ON pos.INVOICEID = inv.INVOICEID) LEFT JOIN COMPANYPRODUCTTAX tax ON pos.TAXTYPE = tax.NAME WHERE inv.DATE BETWEEN $P{DATE_FROM} AND $P{DATE_TO}]]>
